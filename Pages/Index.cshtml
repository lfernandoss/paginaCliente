@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<form class="row g-3">
  <div class="col-md-6">
    <label for="inputEmail4" class="form-label">ID Cliente</label>
    <input type="email" class="form-control" id="inputEmail4">
  </div>
  <div class="col-md-6">
    <label for="inputPassword4" class="form-label">Nome</label>
    <input type="password" class="form-control" id="inputPassword4">
  </div>
  <div class="col-12">
    <label for="inputAddress" class="form-label">RG</label>
    <input type="text" class="form-control" id="inputAddress" placeholder="Escreva seu RG">
  </div>
  <div class="col-12">
    <label for="inputAddress2" class="form-label">CPF</label>
    <input type="text" class="form-control" id="inputAddress2" placeholder="Escreva seu CPF">
  </div>
  <div class="col-md-6">
    <label for="inputCity" class="form-label">Endereço</label>
    <input type="text" class="form-control" id="inputCity">
  </div>
  <div class="col-md-6">
    <label for="inputCity" class="form-label">Produto</label>
    <input type="text" class="form-control" id="inputCity">
  </div>
  
  <div class="col-md-2">
    <label for="inputZip" class="form-label">Preço de venda</label>
    <input type="text" class="form-control" id="inputZip">
  </div>
  <div class="col-12">
    <button type="submit" class="btn btn-primary">Enviar</button>
  </div>
</form>